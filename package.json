{
  "name": "groupe-abc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20 <23"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build",
    "build:ssr": "ng build --configuration=production && ng run groupe-abc:server:production",
    "postbuild:ssr": "node -e \"const fs=require('fs'),p='dist/groupe-abc/server/'; if(fs.existsSync(p+'main.js')) fs.copyFileSync(p+'main.js', p+'main.cjs');\"",
    "serve:ssr": "ng run groupe-abc:serve-ssr:production",
    "start:ssr": "npm run build:ssr && npm run serve:ssr",
    "build:ssr:staging": "ng build --configuration=staging && ng run groupe-abc:server:staging",
    "postbuild:ssr:staging": "node -e \"const fs=require('fs'),p='dist/groupe-abc/server/'; if(fs.existsSync(p+'main.js')) fs.copyFileSync(p+'main.js', p+'main.cjs');\"",
    "serve:ssr:staging": "ng run groupe-abc:serve-ssr:staging",
    "start:ssr:staging": "npm run build:ssr:staging && npm run serve:ssr:staging",
    "serve:ssr:node": "node ssr.mjs",
    "start:ssr:node": "npm run build:ssr && node ssr.mjs",
    "prerender": "ng run groupe-abc:prerender",
    "prerender:staging": "ng run groupe-abc:prerender:staging",
    "deploy-assets:prod": "ng run groupe-abc:deploy-assets:production",
    "deploy-assets:staging": "ng run groupe-abc:deploy-assets:staging",
    "build:prod": "ng build --configuration=production && ng run groupe-abc:server:production && npm run deploy-assets:prod",
    "build:staging": "ng build --configuration=staging && ng run groupe-abc:server:staging && npm run deploy-assets:staging",
    "test": "ng test --watch=false",
    "lint": "node -e \"console.log('Lint non configuré. Exécutez: ng add @angular-eslint/schematics');\"",
    "clean": "rimraf dist .angular"
  },
  "dependencies": {
    "@angular/animations": "18.2.12",
    "@angular/cdk": "18.2.12",
    "@angular/common": "18.2.12",
    "@angular/compiler": "18.2.12",
    "@angular/core": "18.2.12",
    "@angular/forms": "18.2.12",
    "@angular/platform-browser": "18.2.12",
    "@angular/platform-browser-dynamic": "18.2.12",
    "@angular/platform-server": "18.2.12",
    "@angular/router": "18.2.12",
    "@angular/ssr": "18.2.12",
    "animejs": "3.2.2",
    "bootstrap": "5.3.3",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "domino": "^2.1.6",
    "express": "^4.21.2",
    "express-static-gzip": "2.1.8",
    "glob": "11.0.0",
    "gsap": "^3.13.0",
    "node-fetch": "^3.3.2",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zone.js": "0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.2.12",
    "@angular/cli": "18.2.12",
    "@angular/compiler-cli": "18.2.12",
    "@fullhuman/postcss-purgecss": "6.0.0",
    "@types/animejs": "^3.1.12",
    "@types/compression": "1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jasmine": "5.1.0",
    "@types/node": "^20.11.30",
    "cssnano": "7.0.6",
    "jasmine-core": "5.2.0",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ngrok": "5.0.0-beta.2",
    "rimraf": "6.0.1",
    "rollup": "4.24.0",
    "typescript": "5.4.4",
    "vite": "^5.4.14"
  },
  "overrides": {
    "glob": "11.0.0"
  }
}
